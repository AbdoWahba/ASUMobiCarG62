#define F_CPU 1000000UL
#include <avr/io.h>
#include <util/delay.h>
int main(void)
{
 DDRA =0b11111111;
 while(1)
 {
PORTA = 0b00111111;
_delay_ms(1000);

PORTA = 0b00110000; 
_delay_ms(1000);

PORTA = 0b01011011; 
_delay_ms(1000);

PORTA = 0b01001111; 
_delay_ms(1000);

PORTA = 0b01100110; 
_delay_ms(1000);

PORTA = 0b01101101;
_delay_ms(1000);

 PORTA = 0b01111101;
_delay_ms(1000);

 PORTA = 0b00000111; 
_delay_ms(1000);

 PORTA = 0b11111111; 
 _delay_ms(1000);

 PORTA = 0b01101111; 
_delay_ms(1000);

 }
 return 0;
}
